{
  "name": "liftosaur",
  "version": "1.0.0",
  "description": "App for tracking weight lifting progress",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn docs && NODE_ENV=production yarn build:dev && yarn blog",
    "fullbuild": "yarn typecheck && yarn lint && yarn build",
    "publish": "wrangler publish --env production",
    "analyze": "open dist/report.html",
    "docs": "generate-md --layout ./src/docs/layout/ --input ./src/docs/content/ --output ./docs",
    "blog": "eleventy",
    "build:server": "webpack --config webpack.server.config.js",
    "build:dev": "webpack && node esbuild.js",
    "typecheck": "tsc --noEmit --incremental",
    "lint": "eslint '{src,server,test}/**/*.{ts,tsx}' --cache",
    "start": "webpack-dev-server",
    "trust": "sudo security add-trusted-cert -d -r trustRoot -k /Library/Keychains/System.keychain cert/private.crt",
    "start:https": "webpack-dev-server --https --cert cert/private.pem --key cert/private.key --port 8443",
    "start:server": "wrangler dev",
    "migrate": "curl -v -X POST http://local-api.liftosaur.com:8787/api/migrate?key=$KEY",
    "migrate:prod": "curl -v -X POST http://api.liftosaur.com/api/migrate?key=$KEY",
    "synctoprod": "curl -v -X POST http://local-api.liftosaur.com:8787/api/synctoprod?ids=$IDS\\&key=$KEY",
    "synctodev": "curl -v -X POST http://local-api.liftosaur.com:8787/api/synctodev?ids=$IDS\\&key=$KEY",
    "test": "jest"
  },
  "author": "Anton Astashov",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@cloudflare/wrangler": "^1.12.2",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@types/codemirror": "^0.0.96",
    "@types/cookie": "^0.3.3",
    "@types/deepmerge": "^2.2.0",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@udacity/types-service-worker-mock": "^1.1.2",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "cypress": "^5.1.0",
    "cypress-terminal-report": "^2.0.0",
    "esbuild": "^0.8.44",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.7",
    "markdown-styles": "^3.1.10",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "rollbar": "^2.16.2",
    "style-loader": "^1.1.3",
    "tailwindcss": "^1.2.0",
    "tailwindcss-cli": "^0.1.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "types-cloudflare-worker": "^1.1.2",
    "typescript": "^4.1.5",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "wrangler-js": "^0.1.11"
  },
  "dependencies": {
    "codemirror": "^5.54.0",
    "cookie": "^0.4.0",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.2",
    "fp-ts": "^2.6.2",
    "idb-keyval": "^3.2.0",
    "io-ts": "^2.2.4",
    "jsonwebtoken": "^8.5.1",
    "lens-shmens": "^1.0.3",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.10",
    "uplot": "^1.0.9"
  }
}
